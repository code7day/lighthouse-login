{
    "name": "zedu/iws-graphql-login",
    "description": "Laravel Graphql Login for IWS with Lighthouse and Passport",
    "type": "library",
    "require": {
        "php": ">=7.3.0",
        "nuwave/lighthouse": "^3.0|^4.0",
        "laravel/passport": "^7.0|^8.0|^9.0",
        "laravel/socialite": "^4.3",
        "laravel/ui": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Zedu\\IwsGraphqlLogin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zedu\\IwsGraphqlLogin\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [{
        "name": "Edu Flores",
        "email": "edu@iws.pe",
        "homepage": "https://iws.pe",
        "role": "Developer"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Zedu\\IwsGraphqlLogin\\Providers\\IwsGraphqlLoginServiceProvider"
            ]
        }
    }
}
